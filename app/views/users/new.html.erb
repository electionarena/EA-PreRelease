<% @page_title = "Create Subject" %>

<%= link_to("<< Back to List", {:action => 'list'}, :class => 'back-link') %>

<div class="user new">
  <h2>Create User</h2>

  <%= form_for(:user, :url => {:action => 'create'}) do |f| %>
  
    <%= render(:partial => "new_edit", :locals => {:f => f}) %>
    
    <div class="form-buttons">
      <%= submit_tag("Create User") %>
    </div>
    
  <% end %>
</div>
